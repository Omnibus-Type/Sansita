feature kern {
	lookup kern1 {
		pos Ytilde p -50;
		pos Ytilde r -60;
		pos Ygrave p -50;
		pos Ygrave r -60;
		pos Ydieresis p -50;
		pos Ydieresis r -60;
		pos Ycircumflex p -50;
		pos Ycircumflex r -60;
		pos Yacute p -50;
		pos Yacute r -60;
		pos colon zero 30;
		pos Y p -50;
		pos Y r -60;
	subtable;
		pos Lslash @MMK_R_y -20;
		pos X @MMK_R_y -25;
		pos X @MMK_R_o -20;
		pos @MMK_L_y question 30;
		pos @MMK_L_y @MMK_R_period -70;
		pos @MMK_L_V @MMK_R_a -30;
		pos @MMK_L_V @MMK_R_o -31;
		pos F @MMK_R_a -40;
		pos @MMK_L_Y @MMK_R_o -80;
		pos @MMK_L_Y @MMK_R_f -50;
		pos @MMK_L_Y @MMK_R_s -80;
		pos @MMK_L_Y @MMK_R_n -30;
		pos @MMK_L_Y @MMK_R_a -60;
		pos @MMK_L_Y @MMK_R_v -30;
		pos @MMK_L_Y @MMK_R_u -60;
		pos @MMK_L_Y @MMK_R_g -51;
		pos @MMK_L_Y @MMK_R_t -50;
		pos @MMK_L_f @MMK_R_asterisk 151;
		pos @MMK_L_f exclam 162;
		pos @MMK_L_f bracketright 101;
		pos @MMK_L_f parenright 102;
		pos @MMK_L_f question 162;
		pos @MMK_L_T @MMK_R_a -60;
		pos @MMK_L_T @MMK_R_o -51;
		pos @MMK_L_T @MMK_R_y -30;
		pos @MMK_L_T @MMK_R_v -60;
		pos @MMK_L_r colon 30;
		pos @MMK_L_r @MMK_R_v 20;
		pos @MMK_L_r @MMK_R_period -90;
		pos @MMK_L_L @MMK_R_asterisk -80;
		pos @MMK_L_L @MMK_R_y -60;
		pos P @MMK_R_o -30;
		pos @MMK_L_hyphen one -140;
		pos @MMK_L_period @MMK_R_V -80;
		pos @MMK_L_period @MMK_R_Y -100;
		pos @MMK_L_period @MMK_R_T -100;
		pos @MMK_L_asterisk @MMK_R_A -100;
		pos @MMK_L_asterisk @MMK_R_period -200;
		pos @MMK_L_asterisk @MMK_R_s -50;
		pos @MMK_L_asterisk @MMK_R_o -100;
	} kern1;
} kern;
